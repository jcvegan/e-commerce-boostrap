{
  "name": "@jcvegan/boot-commerce",
  "version": "1.0.0",
  "description": "Yet another theme for an e-commerce",
  "main": "index.js",
  "scripts": {
    "build": "webpack --mode=development",
    "watch": "webpack --mode=development --watch",
    "watch:externalServer": "webpack --mode=development --watch --externalServer",
    "bundle": "npm ci && npm run watch",
    "bundle:externalServer": "npm ci && npm run watch:externalServer",
    "production": "cross-env NODE_ENV=production webpack --mode=production",
    "lint-sass": "sass-lint -v -q --format=compact",
    "lint-js": "eslint --ext .js src/js/"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/jcvegan/e-commerce-boostrap.git"
  },
  "keywords": [
    "bootstrap",
    "e-commerce",
    "sass",
    "webpack"
  ],
  "author": "Juan Vega",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/jcvegan/e-commerce-boostrap/issues"
  },
  "homepage": "https://github.com/jcvegan/e-commerce-boostrap#readme",
  "dependencies": {
    "@fortawesome/fontawesome-free": "^5.14.0",
    "autoprefixer": "^9.8.5",
    "bootstrap": "^4.5.0",
    "jquery": "^3.5.1",
    "popper.js": "^1.16.1"
  },
  "devDependencies": {
    "@babel/core": "^7.10.5",
    "@babel/preset-env": "^7.10.4",
    "ajv": "^6.12.3",
    "babel-loader": "^8.1.0",
    "browser-sync": "^2.26.10",
    "browser-sync-webpack-plugin": "^2.2.2",
    "clean-webpack-plugin": "^3.0.0",
    "copy-webpack-plugin": "^6.0.3",
    "cross-env": "^7.0.2",
    "css-loader": "^4.0.0",
    "cssnano": "^4.1.10",
    "eslint": "^7.5.0",
    "eslint-config-airbnb": "^18.2.0",
    "eslint-plugin-import": "^2.22.0",
    "eslint-plugin-jsx-a11y": "^6.3.1",
    "eslint-plugin-react": "^7.20.3",
    "eslint-plugin-react-hooks": "^4.0.8",
    "file-loader": "^6.0.0",
    "html-webpack-plugin": "^4.3.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "mini-css-extract-plugin": "^0.9.0",
    "node-sass": "^7.0.0",
    "optimize-css-assets-webpack-plugin": "^5.0.3",
    "postcss-loader": "^3.0.0",
    "sass-lint": "^1.13.1",
    "sass-loader": "^9.0.2",
    "style-loader": "^1.2.1",
    "terser-webpack-plugin": "^3.0.7",
    "url-loader": "^4.1.0",
    "webpack": "^4.44.0",
    "webpack-cli": "^3.3.12",
    "webpack-dev-server": "^3.11.0"
  }
}
